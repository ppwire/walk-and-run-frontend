item>\r\n\r\n      <v-divider></v-divider>\r\n\r\n      <v-list dense>\r\n        <template v-if=\"role == 'user'\">\r\n          <v-list-item v-for=\"item in items\" :key=\"item.title\" link>\r\n            <v-list-item-icon>\r\n              <v-icon color=\"black\" @click=\"changeCom(item.com)\">{{item.icon}}</v-icon>\r\n            </v-list-item-icon>\r\n\r\n            <v-list-item-content>\r\n              <v-list-item-title @click=\"changeCom(item.com)\">{{ item.title }}</v-list-item-title>\r\n            </v-list-item-content>\r\n          </v-list-item>\r\n        </template>\r\n\r\n        <template v-if=\"role == 'admin'\">\r\n          <v-list-item v-for=\"itemadmin in itemadmins\" :key=\"itemadmin.title\" link>\r\n            <v-list-item-icon>\r\n              <v-icon color=\"black\" @click=\"changeCom(itemadmin.com)\">{{itemadmin.icon}}</v-icon>\r\n            </v-list-item-icon>\r\n\r\n            <v-list-item-content>\r\n              <v-list-item-title @click=\"changeCom(itemadmin.com)\">{{ itemadmin.title }}</v-list-item-title>\r\n            </v-list-item-content>\r\n          </v-list-item>\r\n        </template>\r\n      </v-list>\r\n    </v-navigation-drawer>\r\n\r\n    <component v-bind:is=\"component\"></component>\r\n  </v-app>\r\n</template>\r\n\r\n<script>\r\nimport router from \"../router\";\r\nimport Uploader from \"./uploader.vue\";\r\nimport Homecard from \"./homecard.vue\";\r\nimport Setting from \"./setting.vue\";\r\nimport Work from \"./work.vue\";\r\nimport Sections from \"./section.vue\";\r\nimport Event from \"./event.vue\";\r\nexport default {\r\n  components: {\r\n    uploader: Uploader,\r\n    homecard: Homecard,\r\n    setting: Setting,\r\n    work: Work,\r\n    sections: Sections,\r\n    event: Event\r\n  },\r\n  data() {\r\n    return {\r\n      role: this.$store.getters.role,\r\n      drawer: null,\r\n      component: \"homecard\",\r\n      items: [\r\n        { title: \"Home\", icon: \"mdi-home\", com: \"homecard\", role: \"uni\" },\r\n        {\r\n          title: \"Upload\",\r\n          icon: \"mdi-cloud-upload\",\r\n          com: \"uploader\",\r\n          role: \"user\"\r\n        },\r\n        {\r\n          title: \"Worklist\",\r\n          icon: \"mdi-clipboard-list\",\r\n          com: \"work\",\r\n          role: \"user\"\r\n        },\r\n        {\r\n          title: \"Setting\",\r\n          icon: \"mdi-settings-outline\",\r\n          com: \"setting\",\r\n          role: \"uni\"\r\n        }\r\n      ],\r\n      itemadmins: [\r\n        { title: \"Home\", icon: \"mdi-home\", com: \"homecard\", role: \"uni\" },\r\n        {\r\n          title: \"Setting\",\r\n          icon: \"mdi-settings-outline\",\r\n          com: \"setting\",\r\n          role: \"uni\"\r\n        },\r\n        {\r\n          title: \"Section\",\r\n          icon: \"mdi-account-group\",\r\n          com: \"sections\",\r\n          role: \"admin\"\r\n        },\r\n        {\r\n          title: \"Event\",\r\n          icon: \"mdi-tooltip-text-outline\",\r\n          com: \"event\",\r\n          role: \"admin\"\r\n        }\r\n      ]\r\n    };\r\n  },\r\n  methods: {\r\n    changeCom(com) {\r\n      console.log(com);\r\n      this.component = com;\r\n    },\r\n    logout() {\r\n      this.$store.dispatch(\"destroyToken\").then(router.push({ name: \"login\" }));\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.v-toolbar {\r\n  flex: none;\r\n}\r\n\r\n.logouticon {\r\n  margin: 1%;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}