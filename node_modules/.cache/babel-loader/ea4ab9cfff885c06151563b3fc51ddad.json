         v-model=\"eventdes\"\r\n                          height=\"200\"\r\n                          :rules=\"[v => !!v || 'กรุณาเติมข้อมูลให้ครบ']\"\r\n                          label=\"คำอธิบาย Event\"\r\n                          required\r\n                        ></v-text-field>\r\n                      </v-col>\r\n                      <v-col cols=\"6\">\r\n                        <v-select\r\n                        v-model=\"eventtype\"\r\n                          \r\n                          :rules=\"[v => !!v || 'กรุณาเติมข้อมูลให้ครบ']\"\r\n                          :items=\"['คูณ','เปอร์เซ็น','ปัดเศษ']\"\r\n                          label=\"ประเภท\"\r\n                          required\r\n                        ></v-select>\r\n                      </v-col>\r\n                      <v-col cols=\"6\">\r\n                        <v-text-field\r\n                        v-model=\"eventvalue\"\r\n                          :rules=\"[v => !!v || 'กรุณาเติมข้อมูลให้ครบ']\"\r\n                          label=\"Value\"\r\n                          required\r\n                        ></v-text-field>\r\n                      </v-col>\r\n                    </v-row>\r\n                    <v-row justify=\"center\">\r\n                      <v-date-picker v-model=\"eventdate\"></v-date-picker>\r\n                    </v-row>\r\n                  </v-container>\r\n                </v-form>\r\n              </v-card-text>\r\n              <v-card-actions>\r\n                <v-spacer></v-spacer>\r\n                <v-btn color=\"blue darken-1\" text @click=\"dialog = false\">Close</v-btn>\r\n                <v-btn color=\"blue darken-1\" text @click=\"dialog = false ; submitEvent()\">Save</v-btn>\r\n              </v-card-actions>\r\n            </v-card>\r\n          </v-dialog>\r\n        </v-row>\r\n      </v-row>\r\n    </v-card>\r\n  </v-app>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport subEvent from './subevent.vue'\r\nexport default {\r\n  components:{\r\n    subevent : subEvent\r\n  },\r\n  data: () => ({\r\n    componentkey:0,\r\n    dialog: false,\r\n    eventtitle: null,\r\n    eventdes: null,\r\n    eventtype: null,\r\n    eventvalue: null,\r\n    eventdate: new Date().toISOString().substr(0, 10),\r\n    valid: false\r\n  }),\r\n  methods: {\r\n    async submitEvent() {\r\n\r\n      if(this.eventtype == 'คูณ'){\r\n        this.eventtype = 'M'\r\n      }else if (this.eventtype == 'เปอร์เซ็น'){\r\n        this.eventtype = 'P'\r\n      }else if (this.eventtype == 'ปัดเศษ'){\r\n        this.eventtype = 'C'\r\n      }\r\n      \r\n\r\n      await axios\r\n        .post(\"/admin/insertevent\", {\r\n          eventtitle:this.eventtitle,\r\n          eventdes:this.eventdes,\r\n          eventtype:this.eventtype,\r\n          eventvalue:this.eventvalue,\r\n          eventdate:this.eventdate\r\n        })\r\n        .then(response => {\r\n          console.log(response.data.data);\r\n          this.events = response.data.data;\r\n          this.componentkey++\r\n        })\r\n        .catch(function(error) {\r\n          console.log(error);\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n"],"sourceRoot":"src/components"}]}