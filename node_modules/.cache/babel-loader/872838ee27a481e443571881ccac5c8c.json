AAA,OAAA;AACA,cAAA,OAAA,CAAA,GAAA,CAAA,KAAA,WAAA;AAvDA;AAAA,8CAwDA,KAAA,CACA,IADA,CACA,4BADA,EACA;AACA,gBAAA,WAAA,EAAA,KAAA,WAAA,CAAA,WADA;AAEA,gBAAA,YAAA,EAAA,KAAA,WAAA,CAAA,QAFA;AAGA,gBAAA,WAAA,EAAA,KAAA,aAHA;AAIA,gBAAA,YAAA,EAAA,KAAA,WAAA,CAAA,QAJA;AAKA,gBAAA,aAAA,EAAA,KAAA,WAAA,CAAA,SALA;AAMA,gBAAA,gBAAA,EAAA,KAAA,WAAA,CAAA,YANA;AAOA,gBAAA,YAAA,EAAA,KAAA,MAPA;AAQA,gBAAA,iBAAA,EAAA,KAAA,WAAA,CAAA;AARA,eADA,EAWA,IAXA,CAWA,UAAA,QAAA,EAAA;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,eAbA,EAcA,KAdA,CAcA,UAAA,KAAA,EAAA;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,eAhBA,CAxDA;;AAAA;AAAA;AAAA;;AAAA;AA0EA,cAAA,OAAA,CAAA,GAAA,CAAA,cAAA;;AA1EA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA9BA,CAAA","sourcesContent":["<template>\r\n  <v-app>\r\n    <v-card min-height=\"100vh\" max-height=\"auto\">\r\n      <v-row>\r\n        <v-col md=\"2\"></v-col>\r\n        <v-col md=\"9\">\r\n          <v-card max-width=\"auto\">\r\n            <v-card-title>Uploader</v-card-title>\r\n            <v-container>\r\n              <v-progress-linear color=\"light-blue\" height=\"10\" :value=\"progress\" striped></v-progress-linear>\r\n              <input type=\"file\" @change=\"onFileSelected\" class=\"v-btn\" />\r\n              <v-btn @click=\"onUpload\">Upload</v-btn>\r\n              <v-btn @click=\"sendImage\">Submit</v-btn>\r\n              <v-dialog v-model=\"dialog\" persistent max-width=\"290\">\r\n                <v-card>\r\n                  <v-card-text>Upload Success</v-card-text>\r\n                  <v-card-actions>\r\n                    <v-btn @click=\"close\">Ok</v-btn>\r\n                  </v-card-actions>\r\n                </v-card>\r\n              </v-dialog>\r\n            </v-container>\r\n            <img v-bind:src=\"imageUrl\" />\r\n            <v-form v-model=\"valid\" ref=\"form\">\r\n              <h3>Time</h3>\r\n              <v-select\r\n                :items=\"timeblocks\"\r\n                label=\"Time\"\r\n                solo\r\n                v-model=\"dataimgpack.timedata\"\r\n                :rules=\"[v => !!v || 'Time is required']\"\r\n              ></v-select>\r\n              <h3>Distance</h3>\r\n              <v-select\r\n                :items=\"disblocks\"\r\n                label=\"Distance\"\r\n                solo\r\n                v-model=\"dataimgpack.distancedata\"\r\n                :rules=\"[v => !!v || 'Distance is required']\"\r\n              ></v-select>\r\n              <h3>Pace</h3>\r\n              <v-select\r\n                :items=\"paceblocks\"\r\n                label=\"Pace\"\r\n                solo\r\n                v-model=\"dataimgpack.pacedata\"\r\n                :rules=\"[v => !!v || 'Pace is required']\"\r\n              ></v-select>\r\n              <h3>Speed</h3>\r\n              <v-select\r\n                :items=\"speedblocks\"\r\n                label=\"Speed\"\r\n                solo\r\n                v-model=\"dataimgpack.speeddata\"\r\n                :rules=\"[v => !!v || 'Speed is required']\"\r\n              ></v-select>\r\n              <h3>Calorie</h3>\r\n              <v-select\r\n                :items=\"calblocks\"\r\n                label=\"Calorie\"\r\n                solo\r\n                v-model=\"dataimgpack.caloriedata\"\r\n                :rules=\"[v => !!v || 'Calorie is required']\"\r\n              ></v-select>\r\n\r\n              <h3>Time Started</h3>\r\n              <v-text-field\r\n                label=\"Time Started\"\r\n                solo\r\n                v-model=\"dataimgpack.timestart\"\r\n                :rules=\"[v => !!v || 'Time Started is required']\"\r\n              ></v-text-field>\r\n\r\n              <v-btn @click=\"timepick = true\">\r\n                Pick your date\r\n              </v-btn>\r\n\r\n            </v-form>\r\n          </v-card>\r\n        </v-col>\r\n      </v-row>\r\n    </v-card>\r\n\r\n    <v-dialog v-model=\"timepick\" persistent max-width=\"290\">\r\n      <v-card>\r\n        <v-card-text>Pick Your Time</v-card-text>\r\n        <v-date-picker v-model=\"picker\"></v-date-picker>\r\n        <v-card-actions>\r\n          <v-btn @click=\"timepick = false\">Ok</v-btn>\r\n        </v-card-actions>\r\n      </v-card>\r\n    </v-dialog>\r\n\r\n  </v-app>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      timepick : false,\r\n      picker: new Date().toISOString().substr(0, 10),\r\n      cloudimg: \"https://api.cloudinary.com/v1_1/dd7cchy0p/image/upload\",\r\n      selectedFile: \"\",\r\n      image: null,\r\n      imageUrl: \"\",\r\n      imageCloudUrl: null,\r\n      progress: 0,\r\n      dialog: false,\r\n      timeblocks: [],\r\n      disblocks: [],\r\n      paceblocks: [],\r\n      speedblocks: [],\r\n      calblocks: [],\r\n      valid: false,\r\n      dataimgpack: {\r\n        timestart: null,\r\n        timedata: null,\r\n        distancedata: null,\r\n        pacedata: null,\r\n        speeddata: null,\r\n        caloriedata: null,\r\n        datedata:null\r\n      }\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    async onFileSelected(event) {\r\n      console.log(event.target.files[0]);\r\n      const fileReader = new FileReader();\r\n      await fileReader.addEventListener(\"load\", () => {\r\n        this.imageUrl = fileReader.result;\r\n      });\r\n      await fileReader.readAsDataURL(event.target.files[0]);\r\n\r\n      this.selectedFile = this.imageUrl;\r\n    },\r\n    async onUpload() {\r\n      await axios\r\n        .post(\"/usermanage/insertimguser\", {\r\n          file: this.imageUrl\r\n        })\r\n        .then(response => {\r\n          console.log(response);\r\n          this.dialog = true;\r\n          this.imageCloudUrl = response.data.file;\r\n        })\r\n        .catch(error => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    async close() {\r\n      this.dialog = false;\r\n      if (this.imageCloudUrl != null) {\r\n        console.log(this.imageCloudUrl);\r\n        await axios\r\n          .post(\"/usermanage/ocruserwork\", {\r\n            url: this.imageCloudUrl\r\n          })\r\n          .then(response => {\r\n            console.log(response);\r\n            console.log(response.data.text);\r\n            this.timeblocks = response.data.text;\r\n            this.disblocks = response.data.text;\r\n            this.paceblocks = response.data.text;\r\n            this.speedblocks = response.data.text;\r\n            this.calblocks = response.data.text;\r\n          })\r\n          .catch(function(error) {\r\n            console.log(error);\r\n          });\r\n      } else {\r\n        console.log(\"invalid\");\r\n      }\r\n    },\r\n    async sendImage() {\r\n      axios.defaults.headers.post[\r\n        \"authorization\"\r\n      ] = `Bearer ${this.$store.getters.token}`;\r\n      if (this.$refs.form.validate()) {\r\n        console.log(\"valid\");\r\n        console.log(this.dataimgpack);\r\n        await axios\r\n          .post(\"/usermanage/commituserwork\", {\r\n            userworkcal: this.dataimgpack.caloriedata,\r\n            userworktime: this.dataimgpack.timedata,\r\n            userworkurl: this.imageCloudUrl,\r\n            userworkpace: this.dataimgpack.pacedata,\r\n            userworkspeed: this.dataimgpack.speeddata,\r\n            userworkdistance: this.dataimgpack.distancedata,\r\n            userworkdate:this.picker,\r\n            userworkstarttime:this.dataimgpack.timestart\r\n          })\r\n          .then(function(response) {\r\n            console.log(response);\r\n          })\r\n          .catch(function(error) {\r\n            console.log(error);\r\n          });\r\n      } else {\r\n        console.log(\"invalid form\");\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>"],"sourceRoot":"src/components"}]}